<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #60408b;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testgui.OnlineHistoryController">
   <children>
      <Label fx:id="title" layoutX="261.0" layoutY="14.0" text="Game History" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <ScrollPane layoutY="66.0" prefHeight="280.0" prefWidth="700.0">
         <content>
            <TableView fx:id="historyTable" prefHeight="280.0" prefWidth="700.0" style="-fx-background-color: #ffffff;">
              <columns>
                <TableColumn fx:id="game" maxWidth="116.0" minWidth="116.0" prefWidth="116.0" style="-fx-background-color: #FCF3CF;" text="Game" />
                <TableColumn fx:id="player1" maxWidth="116.0" minWidth="116.0" prefWidth="116.0" style="-fx-background-color: #FCF3CF;" text="Player 1" />
                  <TableColumn fx:id="p1score" maxWidth="116.0" minWidth="116.0" prefWidth="116.0" style="-fx-background-color: #FCF3CF;" text="Player 1 Score" />
                  <TableColumn fx:id="player2" maxWidth="116.0" minWidth="116.0" prefWidth="116.0" style="-fx-background-color: #FCF3CF;" text="Player 2" />
                  <TableColumn fx:id="p2score" maxWidth="116.0" minWidth="116.0" prefWidth="116.0" style="-fx-background-color: #FCF3CF;" text="Player 2 Score" />
                  <TableColumn fx:id="winner" maxWidth="116.0" minWidth="116.0" prefWidth="116.0" style="-fx-background-color: #FCF3CF;" text="Winner" />
              </columns>
            </TableView>
         </content>
      </ScrollPane>
      <Button fx:id="back" layoutX="270.0" layoutY="354.0" mnemonicParsing="false" onAction="#handleBackAction" prefHeight="32.0" prefWidth="161.0" style="-fx-background-radius: 2em; -fx-background-color: #ffffff;" text="Back" textFill="#60408b">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
