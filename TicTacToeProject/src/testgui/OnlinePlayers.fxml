<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" maxHeight="400.0" maxWidth="700.0" minHeight="400.0" minWidth="700.0" prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #60408b;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testgui.OnlinePlayersController">
   <children>
      <Label fx:id="Title" layoutX="259.0" layoutY="14.0" text="Online Players" textFill="#f5f5f5">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <ScrollPane fx:id="ScrollTable" layoutY="66.0" prefHeight="280.0" prefWidth="700.0">
         <content>
            <TableView fx:id="TableP" onMousePressed="#OnMousePressed" prefHeight="280.0" prefWidth="700.0" style="-fx-background-color: #ffffff;">
              <columns>
                <TableColumn fx:id="playerName" maxWidth="350.0" minWidth="350.0" prefWidth="350.0" style="-fx-background-color: #FCF3CF;" text="Player" />
                <TableColumn fx:id="PlayerScore" maxWidth="350.0" minWidth="350.0" prefWidth="350.0" style="-fx-background-color: #FCF3CF;" text="Score" />
              </columns>
            </TableView>
         </content></ScrollPane>
      <Button fx:id="Backbtn" layoutX="110.0" layoutY="354.0" mnemonicParsing="false" onAction="#handleOnlineAction" prefHeight="32.0" prefWidth="161.0" style="-fx-background-radius: 2em; -fx-background-color: #ffffff;" text="Back" textFill="#60408b">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="refreshBtn1" layoutX="420.0" layoutY="354.0" mnemonicParsing="false" onAction="#refreshOnlineAction" prefHeight="32.0" prefWidth="161.0" style="-fx-background-radius: 2em; -fx-background-color: #ffffff;" text="Refresh" textFill="#60408b">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
